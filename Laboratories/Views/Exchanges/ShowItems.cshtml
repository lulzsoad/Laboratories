<table border="1">
    <tr>
        <th>ID</th><th>Name</th><th>Dscription</th><th>Visible?</th>
    </tr>
    @foreach(var item in (List<ItemModel>)ViewData["Items"])
    {
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Description</td>
        <td>@item.IsVisible</td>
    </tr>
    }
</table>